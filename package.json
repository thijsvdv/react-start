{
  "name": "react-start",
  "version": "1.0.0",
  "description": "Starting boilerplate by Thijs",
  "main": "app/index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/thijsvdv/react-start.git"
  },
  "author": "Thijs Van de Velde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thijsvdv/react-start/issues"
  },
  "homepage": "https://github.com/thijsvdv/react-start#readme",
  "dependencies": {
    "compression": "^1.6.1",
    "contentful": "^3.3.5",
    "express": "^4.13.4",
    "if-env": "^1.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "http-server": "^0.9.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
